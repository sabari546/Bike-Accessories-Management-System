<AnchorPane id="AnchorPane" prefHeight="768.0" prefWidth="1312.0" stylesheets="@viewInventoryDesign.css" xmlns="http://javafx.com/javafx/20.0.2" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Admin.ViewAdminInventoryController">
   <children>
      <Label alignment="CENTER" layoutX="585.0" layoutY="23.0" prefHeight="34.0" prefWidth="336.0" text="Inventory management" textAlignment="CENTER">
         <font>
            <Font name="System Bold Italic" size="18.0" />
         </font></Label>
      <AnchorPane layoutX="14.0" layoutY="62.0" prefHeight="691.0" prefWidth="1298.0">
         <children>
            <TableView fx:id="productTable" layoutX="234.0" layoutY="32.0" prefHeight="634.0" prefWidth="1011.0">
              <columns>
                <TableColumn fx:id="colid" prefWidth="109.60003662109375" text="id">
                    <cellValueFactory>
                      <javafx.scene.control.cell.PropertyValueFactory property="Id" />
                     </cellValueFactory>
                </TableColumn>   
                <TableColumn fx:id="colName" prefWidth="262.39996337890625" text="Name">
                    <cellValueFactory>
                          <javafx.scene.control.cell.PropertyValueFactory property="name" /> 
                    </cellValueFactory>
                    </TableColumn> 
                  <TableColumn fx:id="colCategory" minWidth="0.0" prefWidth="157.5999755859375" text="Category">
                      <cellValueFactory>
                          <javafx.scene.control.cell.PropertyValueFactory property="category" /> 
                    </cellValueFactory>
                  </TableColumn>
                  <TableColumn fx:id="colPrice" prefWidth="131.199951171875" text="Price">
                      <cellValueFactory>
                          <javafx.scene.control.cell.PropertyValueFactory property="price" /> 
                    </cellValueFactory>
                  </TableColumn>
                  <TableColumn fx:id="colStock" prefWidth="146.4000244140625" text="Stock">
                      <cellValueFactory>
                          <javafx.scene.control.cell.PropertyValueFactory property="stock" /> 
                    </cellValueFactory>
                  </TableColumn>
                  <TableColumn fx:id="colAddeddate" prefWidth="203.9998779296875" text="StockAddedDate">
                      <cellValueFactory>
                          <javafx.scene.control.cell.PropertyValueFactory property="added_Date" /> 
                    </cellValueFactory>
                  </TableColumn>
              </columns>
            </TableView>
            <BorderPane layoutX="43.0" layoutY="86.0" prefHeight="257.0" prefWidth="164.0">
               <top>
                  <VBox alignment="CENTER_LEFT" prefHeight="481.0" prefWidth="200.0" BorderPane.alignment="CENTER">
                     <children>
                        <Label text="Select Category" />
                        <ComboBox fx:id="categoryComboBox" prefWidth="150.0" promptText="select" />
                        <Pane prefHeight="84.0" prefWidth="210.0" />
                        <Button fx:id="deletedata" mnemonicParsing="false" onAction="#OnDeletebtn" text="Delete" />
                        <Pane prefHeight="78.0" prefWidth="210.0" />
                        <Button fx:id="editbutton" mnemonicParsing="false" onAction="#OnEditbtn" text="Edit" />
                     </children>
                  </VBox>
               </top>
            </BorderPane>
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
